<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/grey1"
    android:orientation="vertical">


    <com.eex.common.view.NewComTitleBar
        android:id="@+id/comtitlebar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:iv_right="@mipmap/financial_right" />


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/grey1"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/layout_10dp"
                    android:layout_marginRight="@dimen/layout_10dp"
                    android:layout_marginBottom="@dimen/layout_30dp"
                    android:background="@color/white"
                    android:orientation="vertical">


                    <LinearLayout
                        android:id="@+id/Withdraw_linea"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_margin="@dimen/layout_10dp"
                        android:background="@color/grey1"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:padding="@dimen/layout_10dp">


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:text="@string/Currency_name"
                            android:textColor="@color/mine_item_text"
                            android:textSize="@dimen/text_12sp" />

                        <TextView
                            android:id="@+id/chargemone_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:text="AA"
                            android:textColor="@color/mine_item_text"
                            android:textSize="@dimen/text_12sp" />


                        <ImageView

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:src="@mipmap/down_triangle" />


                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/layout_20dp"
                        android:orientation="vertical">


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_40dp"
                            android:text="@string/Withdraw_addres"
                            android:textColor="@color/mine_item_text"
                            android:textSize="@dimen/text_12sp" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">


                            <EditText
                                android:id="@+id/Withdraw_copy"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/layout_30dp"
                                android:layout_marginTop="@dimen/layout_5dp"
                                android:layout_weight="1"
                                android:background="@color/grey1"
                                android:hint="@string/Withdraw_copy"
                                android:padding="@dimen/layout_10dp"
                                android:singleLine="true"
                                android:textColor="@color/text_color"
                                android:textColorHint="@color/color_cfd6e6"
                                android:textSize="@dimen/text_12sp" />


                            <ImageView
                                android:id="@+id/withdraw_er"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginLeft="@dimen/layout_20dp"
                                android:padding="@dimen/layout_10dp"
                                android:src="@mipmap/withdraw_er" />

                            <ImageView
                                android:id="@+id/withdraw_addrees"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginLeft="@dimen/layout_20dp"
                                android:layout_marginRight="@dimen/layout_30dp"
                                android:padding="@dimen/layout_10dp"
                                android:src="@mipmap/withdraw_addrees" />


                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/layout_40dp"
                                    android:layout_marginTop="@dimen/layout_20dp"
                                    android:text="@string/Capital_more"
                                    android:textColor="@color/mine_item_text"
                                    android:textSize="@dimen/text_12sp" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/layout_30dp"
                                    android:layout_marginTop="@dimen/layout_5dp"
                                    android:layout_weight="1"
                                    android:background="@color/grey1"
                                    android:orientation="horizontal">


                                    <EditText
                                        android:id="@+id/Withdraw_30"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:textColor="@color/text_color"
                                        android:background="@color/grey1"
                                        android:hint="@string/Withdraw_30"
                                        android:padding="@dimen/layout_10dp"
                                        android:singleLine="true"
                                        android:textColorHint="@color/color_cfd6e6"
                                        android:textSize="@dimen/text_12sp" />


                                </LinearLayout>
                            </LinearLayout>


                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/assets_available1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/layout_40dp"
                                    android:layout_marginTop="@dimen/layout_20dp"
                                    android:text="@string/assets_available1"
                                    android:textColor="@color/color_8fa2cc"
                                    android:textSize="@dimen/text_12sp" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/layout_10dp"
                                    android:layout_marginTop="@dimen/layout_5dp"
                                    android:layout_marginRight="@dimen/layout_20dp"
                                    android:layout_weight="1"
                                    android:background="@color/grey1"
                                    android:orientation="horizontal">


                                    <TextView
                                        android:id="@+id/Capital_edtoMuber"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:background="@color/grey1"
                                        android:text=""
                                        android:textColor="@color/text_color"
                                        android:padding="@dimen/layout_10dp"
                                        android:singleLine="true"
                                        android:textColorHint="@color/color_cfd6e6"
                                        android:textSize="@dimen/text_12sp" />

                                    <TextView
                                        android:id="@+id/Capital_name"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginRight="@dimen/layout_10dp"
                                        android:layout_weight="1"
                                        android:gravity="right"
                                        android:text="AA"
                                        android:textColor="@color/color_8fa2cc"
                                        android:textSize="@dimen/text_12sp" />


                                </LinearLayout>
                            </LinearLayout>


                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/assets_freeze1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/layout_40dp"
                                    android:layout_marginTop="@dimen/layout_20dp"
                                    android:text="@string/assets_freeze1"
                                    android:textColor="@color/mine_item_text"
                                    android:textSize="@dimen/text_12sp" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/layout_30dp"
                                    android:layout_marginTop="@dimen/layout_5dp"
                                    android:layout_weight="1"
                                    android:background="@color/grey1"
                                    android:orientation="horizontal">


                                    <TextView
                                        android:id="@+id/Withdraw_charge_Frozen"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:background="@color/grey1"
                                        android:text="@string/Withdraw_30"
                                        android:padding="@dimen/layout_10dp"
                                        android:textColor="@color/text_color"
                                        android:singleLine="true"
                                        android:textColorHint="@color/color_cfd6e6"
                                        android:textSize="@dimen/text_12sp" />

                                    <TextView

                                        android:id="@+id/Withdraw_charge_name"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginRight="@dimen/layout_10dp"
                                        android:layout_weight="1"
                                        android:gravity="right"
                                        android:text="AA"
                                        android:textColor="@color/color_8fa2cc"
                                        android:textSize="@dimen/text_12sp" />

                                </LinearLayout>
                            </LinearLayout>


                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/Withdraw_charge"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/layout_20dp"
                                    android:layout_marginTop="@dimen/layout_20dp"
                                    android:text="@string/Withdraw_charge"
                                    android:textColor="@color/color_8fa2cc"
                                    android:textSize="@dimen/text_12sp" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/layout_10dp"
                                    android:layout_marginTop="@dimen/layout_5dp"
                                    android:layout_marginRight="@dimen/layout_20dp"
                                    android:layout_weight="1"
                                    android:background="@color/grey1"
                                    android:orientation="horizontal">


                                    <TextView
                                        android:id="@+id/Withdraw_charge_munber"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:background="@color/grey1"
                                        android:text=""
                                        android:padding="@dimen/layout_10dp"
                                        android:textColor="@color/text_color"
                                        android:singleLine="true"
                                        android:textColorHint="@color/color_cfd6e6"
                                        android:textSize="@dimen/text_12sp" />

                                    <TextView
                                        android:id="@+id/Withdraw_charge_set"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginRight="@dimen/layout_10dp"
                                        android:layout_weight="1"
                                        android:gravity="right"
                                        android:text="AA"
                                        android:textColor="@color/color_8fa2cc"
                                        android:textSize="@dimen/text_12sp" />


                                </LinearLayout>
                            </LinearLayout>


                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_30dp"
                            android:layout_marginTop="@dimen/layout_40dp"
                            android:layout_marginRight="@dimen/layout_30dp"
                            android:layout_marginBottom="@dimen/layout_20dp"
                            android:orientation="vertical">

                            <Button
                                android:id="@+id/Capital_money"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/re_shape_btn_jb"
                                android:text="@string/Capital_money"
                                android:textColor="@color/white"
                                android:textSize="@dimen/text_14sp" />


                        </LinearLayout>
                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/grey1"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/layout_10dp"
                    android:layout_marginTop="@dimen/layout_10dp"
                    android:layout_marginRight="@dimen/layout_10dp"
                    android:background="@color/white"
                    android:orientation="vertical">


                    <TextView
                        android:id="@+id/Withdraw_301"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/layout_40dp"
                        android:layout_marginTop="@dimen/layout_20dp"
                        android:text="@string/Withdraw_30"
                        android:textColor="#ffcc3333"
                        android:textSize="@dimen/text_12sp" />

                    <TextView
                        android:id="@+id/Withdraw_1000"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/layout_40dp"
                        android:layout_marginTop="@dimen/layout_10dp"
                        android:text="@string/Withdraw_1000"
                        android:textColor="#ffcc3333"
                        android:textSize="@dimen/text_12sp" />


                    <TextView
                        android:id="@+id/Withdraw_no"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/layout_40dp"
                        android:layout_marginTop="@dimen/layout_10dp"
                        android:layout_marginBottom="@dimen/layout_20dp"
                        android:text="@string/Withdraw_no"
                        android:textColor="#ffcc3333"
                        android:textSize="@dimen/text_12sp" />


                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
    </ScrollView>
</LinearLayout>