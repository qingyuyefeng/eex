<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/appbar_background2">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:background="@color/activity_marginTop_da">
        <TextView
            android:id="@+id/text_time"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:gravity="center"
            android:textColor="@color/tra_main_login_text_gray"
            android:textSize="@dimen/text_12sp"
            android:text="2018-01-02-18:01:10：12"/>
        <TextView
            android:id="@+id/text_money"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:gravity="center"
            android:textColor="@color/tra_main_login_text_gray"
            android:textSize="@dimen/text_12sp"
            android:text="数量"/>
        <TextView
            android:id="@+id/severMoney"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:gravity="center"
            android:textColor="@color/tra_main_login_text_gray"
            android:textSize="@dimen/text_12sp"
            android:text="--"/>
        <TextView
            android:id="@+id/text_type"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:gravity="center"
            android:textColor="@color/tra_main_login_text_gray"
            android:textSize="@dimen/text_12sp"
            android:text="状态"/>
        <TextView
            android:id="@+id/text_beizhu"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:gravity="center"
            android:textColor="@color/tra_main_login_text_gray"
            android:textSize="@dimen/text_12sp"
            android:text="备注"/>
    </LinearLayout>

</LinearLayout>
