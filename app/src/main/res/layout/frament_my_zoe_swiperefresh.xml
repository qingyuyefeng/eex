<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/layout_20dp"
        android:background="@drawable/background_green" />

    <com.eex.common.view.ComTitleBar
        android:id="@+id/comtitlebar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </com.eex.common.view.ComTitleBar>


    <android.support.v4.widget.SwipeRefreshLayout
        android:id="@+id/swipeRefresh"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                 <!--名称-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="79dp"
                    android:background="@color/edt_gray"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:visibility="visible">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/layout_10dp"
                        android:src="@drawable/touxiang" />


                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textview_Name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_10dp"
                            android:text=""
                            android:textColor="@color/background_baise"
                            android:textSize="@dimen/text_16sp" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imgview_type"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/layout_10dp" />

                            <TextView
                                android:id="@+id/textview_NameType"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/layout_10dp"
                                android:text=""
                                android:textColor="@color/persion_background"
                                android:textSize="@dimen/text_14sp" />

                            <TextView
                                android:id="@+id/textview_NameType1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/layout_10dp"
                                android:text=""
                                android:textColor="@color/persion_background"
                                android:textSize="@dimen/text_12sp" />
                        </LinearLayout>
                    </LinearLayout>

                </LinearLayout>
                <!--订单中心-->
                <LinearLayout
                    android:id="@+id/LL_Order"
                    android:layout_width="match_parent"
                    android:layout_height="47dp"
                    android:layout_marginTop="5dp"
                    android:background="@color/edt_gray"
                    android:gravity="center_vertical"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_10dp"
                            android:src="@drawable/dingdanzhongxin" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_10dp"
                            android:text="@string/mini_order"
                            android:textColor="@color/background_baise"
                            android:textSize="@dimen/text_14sp" />
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:layout_marginLeft="@dimen/layout_10dp"
                        android:layout_marginRight="@dimen/layout_10dp"
                        android:background="@color/fenge" />
                </LinearLayout>
                 <!--理财中心-->
                <LinearLayout
                    android:id="@+id/LL_Money"
                    android:layout_width="match_parent"
                    android:layout_height="47dp"
                    android:background="@color/edt_gray"
                    android:gravity="center_vertical"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_10dp"
                            android:src="@drawable/liciazhongxin" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_10dp"
                            android:text="@string/mine_financial_center"
                            android:textColor="@color/background_baise"
                            android:textSize="@dimen/text_14sp" />
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:layout_marginLeft="@dimen/layout_10dp"
                        android:layout_marginRight="@dimen/layout_10dp"
                        android:background="@color/fenge" />
                </LinearLayout>
                  <!--充值提现-->
                <LinearLayout
                    android:id="@+id/LL_Recharge"
                    android:layout_width="match_parent"
                    android:layout_height="47dp"
                    android:background="@color/edt_gray"
                    android:gravity="center_vertical"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_10dp"
                            android:src="@drawable/cq_cz" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_10dp"
                            android:text="@string/mini_recharge"
                            android:textColor="@color/background_baise"
                            android:textSize="@dimen/text_14sp" />
                    </LinearLayout>

                </LinearLayout>
                 <!--实名认证-->
                <LinearLayout
                    android:id="@+id/LL_RealName"
                    android:layout_width="match_parent"
                    android:layout_height="47dp"
                    android:layout_marginTop="5dp"
                    android:background="@color/edt_gray"
                    android:gravity="center_vertical"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_10dp"
                            android:src="@drawable/shimingrenzheng" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_10dp"
                            android:text="@string/mini_realname"
                            android:textColor="@color/background_baise"
                            android:textSize="@dimen/text_14sp" />

                        <LinearLayout
                            android:id="@+id/LLExplain"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/TXtype"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/layout_10dp"
                                android:text="@string/mini_realname_level1"
                                android:textColor="@color/persion_background"
                                android:textSize="@dimen/text_14sp" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/layout_10dp"
                                android:src="@drawable/myzoetll" />
                        </LinearLayout>


                        <TextView
                            android:id="@+id/txName"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/layout_10dp"
                            android:text="@string/mini_realname_continue_level2"
                            android:textColor="@color/persion_lanse"
                            android:textSize="@dimen/text_14sp" />

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:layout_marginLeft="@dimen/layout_10dp"
                        android:layout_marginRight="@dimen/layout_10dp"
                        android:background="@color/fenge" />
                </LinearLayout>
                <!--安全中心-->
                <LinearLayout
                    android:id="@+id/LL_security"
                    android:layout_width="match_parent"
                    android:layout_height="47dp"
                    android:background="@color/edt_gray"
                    android:gravity="center_vertical"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_10dp"
                            android:src="@drawable/anquanzhongxin" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_10dp"
                            android:text="@string/mine_security"
                            android:textColor="@color/background_baise"
                            android:textSize="@dimen/text_14sp" />
                    </LinearLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:layout_marginLeft="@dimen/layout_10dp"
                        android:layout_marginRight="@dimen/layout_10dp"
                        android:background="@color/fenge" />
                </LinearLayout>
                <!--收付款设置-->
                <LinearLayout
                    android:id="@+id/LL_Receivables"
                    android:layout_width="match_parent"
                    android:layout_height="47dp"
                    android:background="@color/edt_gray"
                    android:gravity="center_vertical"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_10dp"
                            android:src="@drawable/shoukuanshezhi" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_10dp"
                            android:text="@string/mini_receivables"
                            android:textColor="@color/background_baise"
                            android:textSize="@dimen/text_14sp" />
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:layout_marginLeft="@dimen/layout_10dp"
                        android:layout_marginRight="@dimen/layout_10dp"
                        android:background="@color/fenge" />
                </LinearLayout>
                <!--提币地址-->
                <LinearLayout
                    android:id="@+id/LL_WithdrawMoney"
                    android:layout_width="match_parent"
                    android:layout_height="47dp"
                    android:background="@color/edt_gray"
                    android:gravity="center_vertical"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_10dp"
                            android:src="@drawable/tibidizhi" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_10dp"
                            android:text="@string/mine_mention_money_address"
                            android:textColor="@color/background_baise"
                            android:textSize="@dimen/text_14sp" />
                    </LinearLayout>
                </LinearLayout>
                <!--邀请返佣-->
                <LinearLayout
                    android:id="@+id/LL_Invitation"
                    android:layout_width="match_parent"
                    android:layout_height="47dp"
                    android:layout_marginTop="5dp"
                    android:background="@color/edt_gray"
                    android:gravity="center_vertical"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_10dp"
                            android:src="@drawable/yaoqingfanyong" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_10dp"
                            android:text="@string/mine_invite_commission"
                            android:textColor="@color/background_baise"
                            android:textSize="@dimen/text_14sp" />
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:layout_marginLeft="@dimen/layout_10dp"
                        android:layout_marginRight="@dimen/layout_10dp"
                        android:background="@color/fenge" />
                </LinearLayout>
                <!--商户昵称-->
                <LinearLayout
                    android:id="@+id/LL_Merchant"
                    android:layout_width="match_parent"
                    android:layout_height="47dp"
                    android:background="@color/edt_gray"
                    android:gravity="center_vertical"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_10dp"
                            android:src="@drawable/shanghuzhongxin" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_10dp"
                            android:text="@string/mini_merchant"
                            android:textColor="@color/background_baise"
                            android:textSize="@dimen/text_14sp" />
                    </LinearLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:layout_marginLeft="@dimen/layout_10dp"
                        android:layout_marginRight="@dimen/layout_10dp"
                        android:background="@color/fenge" />
                </LinearLayout>
                 <!--语言选择-->
                <LinearLayout
                    android:id="@+id/LL_Language"
                    android:layout_width="match_parent"
                    android:layout_height="47dp"
                    android:background="@color/edt_gray"
                    android:gravity="center_vertical"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_10dp"
                            android:src="@mipmap/mine_to_switch" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_10dp"
                            android:text="@string/mini_language"
                            android:textColor="@color/background_baise"
                            android:textSize="@dimen/text_14sp" />
                    </LinearLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:layout_marginLeft="@dimen/layout_10dp"
                        android:layout_marginRight="@dimen/layout_10dp"
                        android:background="@color/fenge" />
                </LinearLayout>

                <!--设置-->
                <LinearLayout
                    android:id="@+id/LL_set"
                    android:layout_width="match_parent"
                    android:layout_height="47dp"
                    android:background="@color/edt_gray"
                    android:gravity="center_vertical"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_10dp"
                            android:src="@drawable/shezhi" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_10dp"
                            android:text="@string/mine_setting"
                            android:textColor="@color/background_baise"
                            android:textSize="@dimen/text_14sp" />
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>
        </ScrollView>
    </android.support.v4.widget.SwipeRefreshLayout>

</LinearLayout>
